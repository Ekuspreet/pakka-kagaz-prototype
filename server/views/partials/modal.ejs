<dialog id="<%= modalID %>" class="modal">
  <div class="modal-box">
    <h3 class="text-lg font-bold"><%= modalTitle %></h3>
    <p class="py-4"><%= modalMessage %></p>
    <div class="modal-action">
      <form method="dialog">
        <!-- if there is a button in form, it will close the modal -->
        <button class="btn">Close</button>
      </form>
    </div>
  </div>
</dialog>
